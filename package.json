{
  "name": "gatsby-source-ravendb",
  "version": "1.1.0",
  "description": "A Gatsby source plugin to fetch data from RavenDB",
  "authors": [
    "Andrew Grass <andygrass92@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/theandrewgrass/gatsby-source-ravendb/issues"
  },
  "dependencies": {
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "babel-preset-gatsby-package": "^2.17.0",
    "cross-env": "^7.0.3",
    "jest": "^28.1.1"
  },
  "peerDependencies": {
    "gatsby": "^4.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/__tests__/helpers/"
    ]
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "raven",
    "ravendb"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/theandrewgrass/gatsby-source-ravendb.git"
  },
  "scripts": {
    "build": "babel src --out-dir .",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel src --watch --out-dir .",
    "test": "jest",
    "clean": "git clean -xfd -e node_modules"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
